public String convert15to18DigitId(String fifteenDigit)
/*{
   if (fifteenDigit?.length()==15 ) {
        return (Id)fifteenDigit;
    }
    return '-1';
}*/
{
   if(fifteenDigit ==null){
       return null;
   }else if(fifteenDigit.length() !=15){
       return '-1';
   }else{
       Id id15=fifteenDigit;
       String eight18=id15;
       return eight18;
          }

}       